{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/config",
      "dest": "/api/cfg.py"
    },
    {
      "src": "/api/lexer",
      "dest": "/api/cgmalexer.py"
    },
    {
      "src": "/api/parser",
      "dest": "/api/cgmaparser.py"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}