{
    "version": 2,
    "builds": [
      {
        "src": "api/**/*.py",
        "use": "@vercel/python"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/lexer",
        "dest": "/api/lexer.py"
      },
      {
        "src": "/api/parser",
        "dest": "/api/parser.py"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ]
  }