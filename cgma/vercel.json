{
    "version": 2,
    "builds": [
      {
        "src": "cgma/api/**/*.py",
        "use": "@vercel/python"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "cgma/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ]
  }